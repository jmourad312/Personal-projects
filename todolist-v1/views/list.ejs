<%- include("header") -%> 

    <div id="heading" class="box">
        <h1><%=listTitle%></h1>
    </div>
    <div class="box">
        <% for (let i = 0; i < newListItems.length; i++) { %>
            <div class="item">
                <input type="checkbox" name="" id="">
                <p><%= newListItems[i]%></p>
            </div>
        <% }; %>
        <form class="item" action="/" method="post">
            <input type="text" name="newItem" id="inp1" placeholder="New item" autocomplete="off">
            <button type="submit" name="list" value=<%= listTitle %> >+</button>
        </form>
    </div>

<%- include("footer") -%> 